<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Email</th>
      <th>Username</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %>
